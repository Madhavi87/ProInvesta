                                <td>@Html.DisplayFor(modelItem => item.Interest_Rate_Range)
                                </td>
                                <td>@Html.DisplayFor(modelItem => item.Processing_Fee_Range)
                                </td>
                                <td>@Html.DisplayFor(modelItem => item.Loan_Amount)</td>
                                <td>@Html.DisplayFor(modelItem => item.Tenure_Range)</td>
                            </tr>
                          }
                        </tbody>
                    </table>
                      <div class="row">
